import java.util.Scanner;

public class Team4 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int n= sc.nextInt();
		int count=0;
		int p=0;
		int q=0;
		int r=0;
		for(int i=0; i<n;i++) {
			p=sc.nextInt();
			q=sc.nextInt();
			r=sc.nextInt();
			if(p+q+r >=2) {
            	count++;
            }

		}
                        
            System.out.println(count);
	}
	

}
